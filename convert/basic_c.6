; bra lda_far_ram1

lda_far_ram1                                              
                 php                                      ; save .c
                 phz                                      ; save registers
                 phx                                      
                 tax                                      ; pointer
                 ldz var_bank                             ; RAM1
                 lda 1,x                                  ; check to see if pointer points to "common"
                 cmp #$20                                 
                 bcs _local_1005_10                       ; branch if not
                 ldz text_bank                            ; else select RAM0

_local_1005_10   jsr _lda_far                             ; LDA (.x),Y from bank .z
                 plx                                      
                 plz                                      
                 plp                                      ; restore .c
                 and #$ff                                 ; set processor status per byte fetched
                 rts                                      

; .page
