; C65 BASIC Indirect Load Subroutines

lda_far_ram0                                              
                 phz                                      ; save registers
                 phx                                      
                 tax                                      ; pointer
                 ldz text_bank                            ; RAM0
                 jsr _lda_far                             ; LDA (.x),Y from bank .z
                 plx                                      
                 plz                                      
                 and #$ff                                 ; set processor status per byte fetched
                 rts                                      



