                lda 0,x                                 ; msb of ptr to string
                sta tempf2+1                            
                dex                                     
                lda 0,x                                 ; lsb of ptr to string
                sta tempf2                              
                dex                                     
                lda 0,x                                 ; string length
                pha                                     ; save for later test
                clc                                     
                adc tempf2                              ; want ptr to string's backpointer
                sta tempf2                              
                bcc l167_1                              
                inc tempf2+1                            
